<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.huhyn.swissstyle.PrimaryController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose Round" />
      <ComboBox fx:id="rounds" prefWidth="150.0" />
      <ScrollPane prefHeight="250.0" prefWidth="818.0">
         <content>
            <ListView prefHeight="302.0" prefWidth="803.0" />
         </content>
      </ScrollPane>
      <Label text="Primary View" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
      <Button mnemonicParsing="false" onAction="#switchToHome" text="home" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
